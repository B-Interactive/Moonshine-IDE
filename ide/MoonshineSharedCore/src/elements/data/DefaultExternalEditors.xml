<?xml version="1.0" encoding="UTF-8" ?>
<config>
	<editors>
		<editor isWindows="true" isMac="true">
			<title>VIM</title>
			<defaultLocation>
				<windows>$programFiles/Vim/vim82/gvim.exe</windows>
				<macos>/Applications/MacVim.app</macos>
			</defaultLocation>
			<defaultArguments>
				<windows>-R</windows>
				<macos/>
			</defaultArguments>
			<website>https://www.vim.org/download.php</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>SublimeText</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/Sublime Text.app</macos>
			</defaultLocation>
			<website>https://www.sublimetext.com/3</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>TextMate</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/TextMate.app</macos>
			</defaultLocation>
			<website>https://macromates.com</website>
		</editor>
		<editor isWindows="true" isMac="true">
			<title>Visual Studio Code</title>
			<defaultLocation>
				<windows>$programFiles/Visual Studio Code.exe</windows>
				<macos>/Applications/Visual Studio Code.app</macos>
			</defaultLocation>
			<defaultArguments>
				<windows>--disable-extensions</windows>
				<macos>--disable-extensions</macos>
			</defaultArguments>
			<website>https://code.visualstudio.com</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>IAWriter</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/iA Writer.app</macos>
			</defaultLocation>
			<website>https://ia.net/downloads</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>Prettify XML</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/MacVIM.app</macos>
			</defaultLocation>
			<website>https://adoptopenjdk.net/</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>Emacs</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/Emacs.app</macos>
			</defaultLocation>
			<website>https://www.gnu.org/software/emacs</website>
		</editor>
		<editor isWindows="false" isMac="true">
			<title>BBEdit</title>
			<defaultLocation>
				<windows/>
				<macos>/Applications/BBEdit.app</macos>
			</defaultLocation>
			<website>https://www.barebones.com/products/textwrangler/download.html</website>
		</editor>
		<editor isWindows="true" isMac="false">
			<title>NotePad++</title>
			<defaultLocation>
				<windows>$programFiles/Notepad++/notepad++.exe</windows>
				<macos/>
			</defaultLocation>
			<website>https://notepad-plus-plus.org/downloads/</website>
		</editor>
		<editor isWindows="true" isMac="false">
			<title>UltraEdit</title>
			<defaultLocation>
				<windows>$programFiles/IDM Computer Solutions/UltraEdit/uedit64.exe</windows>
				<macos/>
			</defaultLocation>
			<website>https://www.ultraedit.com/</website>
		</editor>
	</editors>
</config>